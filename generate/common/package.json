      "test-e2e": "npx playwright test"
   },
   "dependencies": {
      "react": "latest",
      "react-dom": "latest",
      "ajaxness": "latest",
      "zlib": "latest"
   },
   "devDependencies": {
      "bun-types": "latest",
      "@playwright/test": "latest",
      "@types/css": "latest",
      "@types/node": "latest",
      "@types/react": "latest",
      "playwright": "latest",
      "typescript": "latest"
   }
}