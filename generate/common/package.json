{
   "type": "module",
   "name": "PROJECT-TEMPLATE-NAME",
   "module": "index.ts",
   "scripts": {
      "start": "bun --hot run index.tsx",
      "debug": "bun run poc/index.tsx",
      "build": "clear; bunx tsc-silent -p tsconfig.json --suppress 2786,1206@ 2339@/src/bundle/bundle.ts",
      "tests": "bun run test-ut; bun run test-e2e",
