{
  "type": "module",
  "module": "index.ts",
  "author": "JonathanSR <jsenaribeiro@gmail.com>",
  "scripts": {
    "check": "eslint src/** --no-ignore",
    "build": "rm -rf tsconfig.tsbuildinfo; bunx tsc -p tsconfig.json",
    "maker": "rm -rf tsconfig.tsbuildinfo; bunx tsc-silent -p tsconfig.json --suppress 2786,1206@ 2339@/src/bundle/bundle.ts",
    "clear": "rm -rf node_modules package-lock.json bun.lockb; bun i; bun run build",
    "fresh": "bun update @reactful/extensions @reactful/commons @reactful/server",
    "deploy": "bun run fresh; rm -rf tsconfig.npm.tsbuildinfo npm; mkdir npm; cp package.npm.json npm/package.json; bunx tsc -p tsconfig.npm.json; cd npm; npm publish --access public; cd .."
  },
  "dependencies": {
    "@reactful/commons": "latest",
    "@reactful/extensions": "latest",
    "@reactful/server": "^0.0.130",
    "css": "^3.0.0",
    "html-format": "^1.1.6",
    "jsdom": "^23.0.1",
    "jsonwebtoken": "latest",
    "marked": "^11.1.1",
    "os": "^0.1.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "zlib": "^1.0.5"
  },
  "devDependencies": {
    "@happy-dom/global-registrator": "^13.3.4",
    "@types/css": "^0.0.37",
    "@types/node": "^20.11.10",
    "@types/react": "^18.2.45",
    "bun-types": "^1.0.20",
    "eslint": "^8.56.0",
    "playwright": "^1.41.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "whatwg-mimetype": "latest"
  }
}